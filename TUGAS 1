#include <iostream>

using namespace std;

int main()
{
 	
	int bil_ganjil, bil_genap;
	
	cout << "Masukan Bilangan ganjil : ";
	cin >> bil_ganjil;
	cout << endl;
	
	cout << "Masukan bilangan genap : ";
	cin >> bil_genap;
	cout << endl;
	
	if (bil_ganjil %2==1 && bil_genap %2==0){
		
		cout << "kedua angka adalah bilangan ganjil dan genap ";
		
	}else {
		
		cout << " kedua angka bukan bilangan ganjil dan genap ";
        	
	} else if {
		
		cout << " Adalah bilangan ganjil";
		
		} else if {
			
			cout << " Adalah bilangan genap";
		} 
			
		
	
	return 0;
}
